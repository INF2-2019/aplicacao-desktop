<BorderPane prefHeight="421.0" prefWidth="553.0" stylesheets="@../principal/styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="relatorio10.Relatorio10Controller">
